#!/bin/bash

INPUT_DIR=$1
OUTPUT_FILE=$2

python defense_combine_ensemble.py \
    --input_dir "${INPUT_DIR}" \
    --output_file "${OUTPUT_FILE}" \
    --checkpoint_1 /checkpoints/senet154_finetune_2.pth.tar   \
    --checkpoint_2 /checkpoints/densenet121_finetune_2.pth.tar   \
    --checkpoint_3 /checkpoints/mobilenet_v2_finetune_2.pth.tar  \
    --checkpoint_4 /checkpoints/resnet18.pth.tar  \
    --checkpoint_5 /checkpoints/inceptionv4_finetune_2.pth.tar  \
    --checkpoint_6 /checkpoints/inceptionresnetv2_finetune_2.pth.tar 
 

    
